  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Design is a collection of objects structured as a directed graph with an",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "optional explicit single parent-child hierarchy. Design represents creator’s",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "idea or a model and its historical evolution. Each state – version snapshot –",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "of the design is represented by a ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/PoieticCore\/documentation\/PoieticCore\/Frame",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ", which is somewhat analogous",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "to a movie frame.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "design-frame-object",
              "type" : "image"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Each object is uniquely identified by an ID. Version of an object is",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/PoieticCore\/documentation\/PoieticCore\/ObjectSnapshot",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ", which contains object’s properties. When working with",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the design, the object ID is the primary object handle.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Each design is assigned a ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/PoieticCore\/documentation\/PoieticCore\/Metamodel",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ", which defines types of objects",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/PoieticCore\/documentation\/PoieticCore\/ObjectType",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " and constraints ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/PoieticCore\/documentation\/PoieticCore\/Constraint",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
  ],
